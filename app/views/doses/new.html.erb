<h1>Doses#new</h1>
<p>Find me in app/views/doses/new.html.erb</p>

<%= form_for [@cocktail, @dose] do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :ingredient %>
  <%= f.collection_select :ingredient_id, Ingredient.all, :id, :name %>

  <%= f.submit %>
<% end %>
